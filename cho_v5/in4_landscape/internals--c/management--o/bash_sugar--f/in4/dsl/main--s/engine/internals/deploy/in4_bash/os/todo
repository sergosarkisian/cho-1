VDI     
     'qemu-img', 'convert', '-f', 'raw',                 'target_dir/some-disk-image.x86_64-1.2.3.raw', '-O', 'vdi',                 'target_dir/some-disk-image.x86_64-1.2.3.vdi'
     
VHD
   'qemu-img', 'convert', '-f', 'raw',                 'target_dir/some-disk-image.x86_64-1.2.3.raw', '-O', 'vpc',                 'target_dir/some-disk-image.x86_64-1.2.3.vhd'
   
       'qemu-img', 'convert', '-f', 'raw',                  'target_dir/some-disk-image.x86_64-1.2.3.raw', '-O', 'vpc',                 '-o', 'subformat=fixed',                 'target_dir/some-disk-image.x86_64-1.2.3.vhdfixed'
